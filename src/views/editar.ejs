  <body>
      <!-- HEADER -->
      <%- include("partials/header.ejs") %> 
 <!-- --------------Dark Mode----------------- -->

 <%- include("partials/darkMode.ejs") %> 
 <!-- -----------Fin Dark Mode---------------- -->
        <main id="main-dark">
            <div>
                <h1>Editar Producto</h1>
                <form action="#" method="get"   style="margin-top:5.5em">
                    <div class='form caja-agregar'>
                      <label for="">Nombre<input class='input style-admin' type="text" name="nombre-producto" required placeholder="Nombre del producto"> </label>
                      <label for="">Descripción<textarea id="" cols="30" rows="10" class='input style-admin'  name="descripción-producto" ></textarea></label>
                      <label for="">Costo<input class='input style-admin' type="number" name="costo-producto" required placeholder="Costo del producto"> </label>
                      <label for="">Imagen<input class='input style-admin' type="file" name="imagen-producto" multiple required placeholder=none> </label>
                    </div>
                    <div  class='form'>
                        <input type="submit" class='btn-admin style-admin' value="Editar">
                    </div>
                </form>
            </div>

        </main>
    
        <!-------------- FOOTER ------------------------>
       <%- include("partials/footer.ejs") %> 
       
 <script src="/javascripts/darkMode.js"></script>
 <script src="/javascripts/home.js"></script>
